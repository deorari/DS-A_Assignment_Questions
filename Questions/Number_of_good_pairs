 int numIdenticalPairs(vector<int>& nums) {
        map<int,int>m;
        int sum=0;
        for(int i=0;i<nums.size()-1;i++)
        {
            for(int j=i+1;j<nums.size();j++)
            {
                if(nums[i]==nums[j])
                {
                    sum++;
                }
            }
        }
        return sum;
    }
